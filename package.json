{
    "name": "vscode-theme",
    "displayName": "Webstorm NewUI Theme",
    "version": "0.0.1",
    "private": true,
    "description": "WebStorm Theme and Icon theme for Visual Studio Code",
    "keywords": ["webstorm", "theme", "icons"],
    "publisher": "yx1126",
    "scripts": {
        "publish": "vsce publish --no-dependencies",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix"
    },
    "devDependencies": {
        "@reallyx/front-config": "^0.0.9",
        "eslint": "^9.16.0"
    },
    "engines": {
        "vscode": "*"
    },
    "categories": ["Themes"],
    "contributes": {
        "themes": [
            {
                "label": "Webstorm NewUI Theme",
                "uiTheme": "vs-dark",
                "path": "./themes/webstorm-theme.json"
            }
        ],
        "iconThemes": [
            {
                "id": "Webstorm NewUI Icon Theme",
                "label": "Webstorm NewUI Icon Theme",
                "path": "./theme-icon/webstorm-icon.json"
            }
        ]
    }
}